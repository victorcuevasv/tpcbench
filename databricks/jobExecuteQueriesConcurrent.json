{
  "name": "Spark execute TPC-DS queries concurrently",
  "new_cluster": {
  	"spark_version": "5.3.x-scala2.11",
  	"aws_attributes": {
    	"zone_id": "us-west-2b",
        "first_on_demand": 1,
        "availability": "SPOT_WITH_FALLBACK"
    },
    "node_type_id": "i3.xlarge",
    "spark_env_vars": {
    	"PYSPARK_PYTHON": "/databricks/python3/bin/python3"
    },
    "enable_elastic_disk": false,
    "num_workers": 2
  },
  "timeout_seconds": 0,
  "spark_jar_task": {
  	"jar_uri": "project/targetsparkcluster/client-1.0-SNAPSHOT-jar-with-dependencies.jar",
    "main_class_name": "org.bsc.dcc.vcv.ExecuteQueriesConcurrentSpark",
    "parameters": 
    	["/dbfs/data", "results", "plans",
    	"/dbfs/FileStore/job-jars/project/targetsparkcluster/client-1.0-SNAPSHOT-jar-with-dependencies.jar",
    	"spark", "2", "1954", "true", "true"
    	]
  }
}

